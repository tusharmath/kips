{
  "defaultSeverity": "error",
  "extends": ["tslint:all", "tslint-config-prettier"],
  "rules": {
    "typedef": [true, "call-signature", "parameter", "arrow-call-signature"],
    "no-parameter-properties": false,
    "no-implicit-dependencies": [true, "dev"],
    "completed-docs": [
      true,
      {
        "enums": true,
        "functions": true,
        "methods": true,
        "properties": true,
        "classes": true,
        "enum-members": true,
        "interfaces": true,
        "namespaces": true,
        "types": true,
        "variables": true
      }
    ]
  },
  "rulesDirectory": []
}
